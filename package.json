{
  "name": "sequelize-i18n",
  "version": "2.1.0",
  "description": "Easy internalization using Sequelize",
  "scripts": {
    "format": "concurrently \"yarn prettier:format\" \"yarn eslint:format\"",
    "prettier:format": "prettier --write \"**/*.md\"",
    "eslint:format": "yarn lint --fix",
    "lint": "eslint -c ./.eslintrc.js --ignore-path ./.eslintignore *.js \"**/*.js\"",
    "test": "mocha ./test/*.js --require @babel/register"
  },
  "dependencies": {
    "braces": "3.0.2",
    "chai-like": "1.1.1",
    "chai-things": "0.2.0",
    "core-js": "3",
    "lodash": "4.12.0",
    "mysql": "2.10.2",
    "sequelize": "5.8.5"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "@types/mocha": "false7.0.2",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "concurrently": "5.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "mocha": "6.1.4",
    "prettier": "2.0.5",
    "sqlite3": "4.0.8"
  }
}
